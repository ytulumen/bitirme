      <TextField fx:id="name" layoutX="92.0" layoutY="131.0" promptText="Name" stylesheets="@../css/gui.css" />
      <TextField fx:id="surname" layoutX="92.0" layoutY="172.0" promptText="Surname" stylesheets="@../css/gui.css" />
      <PasswordField fx:id="password" layoutX="92.0" layoutY="213.0" promptText="Password" stylesheets="@../css/gui.css" />
      <TextField fx:id="identityNumber" layoutX="92.0" layoutY="86.0" promptText="ID number" stylesheets="@../css/gui.css" />
