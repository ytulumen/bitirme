<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="800.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.AFewWorks.AddVoterController">
   <children>
      <TextField fx:id="name" layoutX="79.0" layoutY="151.0" promptText="Name" stylesheets="@../css/gui.css" />
      <TextField fx:id="surname" layoutX="79.0" layoutY="192.0" promptText="Surname" stylesheets="@../css/gui.css" />
      <PasswordField fx:id="password" layoutX="79.0" layoutY="233.0" promptText="Password" stylesheets="@../css/gui.css" />
      <TextField fx:id="identityNumber" layoutX="79.0" layoutY="106.0" promptText="ID number" stylesheets="@../css/gui.css" />
      <TextField fx:id="street" layoutX="303.0" layoutY="106.0" promptText="Street" stylesheets="@../css/gui.css" />
      <TextField fx:id="number" layoutX="303.0" layoutY="151.0" promptText="Number" stylesheets="@../css/gui.css" />
      <TextField fx:id="town" layoutX="303.0" layoutY="192.0" promptText="Town" stylesheets="@../css/gui.css" />
      <TextField fx:id="city" layoutX="303.0" layoutY="235.0" promptText="City" stylesheets="@../css/gui.css" />
      <Label layoutX="214.0" layoutY="14.0" prefHeight="17.0" prefWidth="149.0" text="ADD VOTER" />
      <Button fx:id="submitButton" layoutX="425.0" layoutY="305.0" mnemonicParsing="false" onAction="#submitVoter" text="Submit" />
      <Button layoutX="26.0" layoutY="44.0" mnemonicParsing="false" onAction="#back" text="Back" />
   </children>
</AnchorPane>
